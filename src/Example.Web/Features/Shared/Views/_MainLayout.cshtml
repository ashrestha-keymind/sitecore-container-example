<!doctype html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <title>Sitecore container example </title>
    <link href="~/wwwroot/lib/bootstrap/css/bootstrap.min.css" rel="stylesheet">
    <link href="~/wwwroot/css/site.style.css" rel="stylesheet">
</head>
<body>
    <main>
        <div class="container py-4">
            @Html.Sitecore().Placeholder("header")
            
            @Html.Sitecore().Placeholder("main")

            @Html.Sitecore().Placeholder("footer")
        </div>
    </main>
    <script src="~/wwwroot/lib/bootstrap/js/bootstrap.bundle.min.js"></script>
</body>
</html>
